{
  "name": "RepeatableRandom",
  "version": "0.1.0",
  "summary": "Repeatable Random order for XCTests",
  "description": "Execute Xcode Tests in a random order to detect interdependency of tests.\nOrder can be repeated to reproduce and debug.",
  "homepage": "https://github.com/paulz/RepeatableRandom",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Paul Zabelin": "paulz@users.noreply.github.com"
  },
  "source": {
    "git": "https://github.com/paulz/RepeatableRandom.git",
    "tag": "0.1.0"
  },
  "social_media_url": "https://twitter.com/iospaulz",
  "platforms": {
    "osx": "10.11",
    "ios": "12.0"
  },
  "source_files": "RepeatableRandom/Classes/**/*",
  "frameworks": "XCTest",
  "swift_versions": "5.0",
  "pod_target_xcconfig": {
    "MACOSX_DEPLOYMENT_TARGET": "$(inherited)",
    "IPHONEOS_DEPLOYMENT_TARGET": "$(inherited)",
    "ARCHS": "$(inherited)"
  },
  "user_target_xcconfig": {
    "ARCHS": "$(inherited)"
  },
  "swift_version": "5.0"
}
